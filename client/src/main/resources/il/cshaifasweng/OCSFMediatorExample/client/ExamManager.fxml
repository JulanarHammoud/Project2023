            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <TableView fx:id="Etable" layoutY="25.0" prefHeight="371.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="IdCode" prefWidth="109.0" text="Exam Code" />
          <TableColumn fx:id="subjectExam" prefWidth="109.0" text="subject" />
            <TableColumn fx:id="teacherExam" prefWidth="130.0" text="teacher" />
            <TableColumn fx:id="NumOfQuestions" minWidth="0.0" prefWidth="133.0" text="num of questions" />

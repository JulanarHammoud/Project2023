<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controller.ShowExamManagerController">
   <children>
      <AnchorPane fx:id="testshowmanager" prefHeight="400.0" prefWidth="600.0">
         <children>
            <ImageView fitHeight="600.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@student.jpg" />
               </image>
            </ImageView>
            <Label fx:id="examTime" layoutX="43.0" layoutY="52.0" prefHeight="31.0" prefWidth="613.0" text="Exam time">
               <font>
                  <Font name="System Italic" size="21.0" />
               </font></Label>
            <Button layoutX="60.0" layoutY="2.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="44.0" text="Back" textFill="#10751d">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Button>
            <Label fx:id="sub" layoutX="43.0" layoutY="83.0" prefHeight="31.0" prefWidth="604.0" text="Subject">
               <font>
                  <Font name="System Italic" size="21.0" />
               </font></Label>
            <Label fx:id="noteStudent" layoutX="43.0" layoutY="114.0" prefHeight="32.0" prefWidth="622.0" text="Note Student">
               <font>
                  <Font name="System Italic" size="22.0" />
               </font></Label>
            <Button layoutY="2.0" mnemonicParsing="false" onAction="#LogOut" prefHeight="25.0" prefWidth="57.0" text="LogOut" textFill="#ab1b1b">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font></Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controller.StudentsExamsController">
    <children>
      <ImageView fitHeight="600.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@dreamers.png" />
         </image>
      </ImageView>
        <TableView fx:id="table" layoutX="29.0" layoutY="51.0" prefHeight="400.0" prefWidth="107.0">
            <columns>
                <TableColumn fx:id="name" prefWidth="124.0" text="Students" />
            </columns>
        </TableView>
      <ScrollPane layoutX="155.0" layoutY="52.0" prefHeight="400.0" prefWidth="515.0">
         <content>
            <AnchorPane fx:id="pane" prefHeight="370.0" prefWidth="505.0" />
         </content></ScrollPane>
      <Button fx:id="approve" layoutX="570.0" layoutY="15.0" mnemonicParsing="false" onAction="#approve" text="Approve" textFill="#a19c22">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Button>
      <Button fx:id="back2" layoutX="95.0" layoutY="15.0" mnemonicParsing="false" onAction="#backaction" text="Back" textFill="#0f5b0a">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Button>
      <Button fx:id="logout1" layoutX="26.0" layoutY="15.0" mnemonicParsing="false" onAction="#logoutactio" text="LogOut" textFill="#860000">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Button>

    </children>

</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="499.0" prefWidth="696.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controller.PrimaryController">
   <children>
      <StackPane fx:id="ok" prefHeight="516.0" prefWidth="730.0">
         <children>
            <ImageView fitHeight="459.0" fitWidth="692.0" opacity="0.9" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@school%20image.jpg" />
               </image>
            </ImageView>
            <Button fx:id="start_button" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" onAction="#start" text="Click Here To Start" textFill="#0b5b15" textOverrun="WORD_ELLIPSIS">
               <StackPane.margin>
                  <Insets top="25.0" />
               </StackPane.margin>
               <font>
                  <Font name="Arial Narrow Bold" size="20.0" />
               </font>
            </Button>
            <Label prefHeight="37.0" prefWidth="539.0" text="Welocme to Our High School Of Dreamers " textFill="#0b5b15">
               <font>
                  <Font name="System Bold Italic" size="27.0" />
               </font>
               <StackPane.margin>
                  <Insets top="-80.0" />
               </StackPane.margin>
            </Label>
         </children>
      </StackPane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>

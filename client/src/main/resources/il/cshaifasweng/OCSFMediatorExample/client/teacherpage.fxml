<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="376.0" prefWidth="535.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controller.TeacherController">
   <children>
      <ImageView fitHeight="520.0" fitWidth="720.0" layoutX="-3.0" layoutY="-6.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@student.jpg" />
         </image>
      </ImageView>
      <Button fx:id="questions" layoutX="50.0" layoutY="81.0" mnemonicParsing="false" onAction="#questionaction" prefHeight="58.0" prefWidth="138.0" text="Questions" textFill="#2c3030">
         <font>
            <Font name="Bell MT" size="23.0" />
         </font>
      </Button>
      <Label layoutX="50.0" layoutY="14.0" text="Teacher:" textFill="#c9c9c9">
         <font>
            <Font name="Baskerville Old Face" size="43.0" />
         </font>
      </Label>
      <Button fx:id="exams" layoutX="50.0" layoutY="172.0" mnemonicParsing="false" onAction="#examsaction" prefHeight="58.0" prefWidth="138.0" text="Exams" textFill="#2c3030">
         <font>
            <Font name="Bell MT" size="23.0" />
         </font>
      </Button>
      <Button fx:id="grades" layoutX="50.0" layoutY="257.0" mnemonicParsing="false" onAction="#gradesaction" prefHeight="58.0" prefWidth="138.0" text="Grades" textFill="#2c3030">
         <font>
            <Font name="Bell MT" size="23.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
